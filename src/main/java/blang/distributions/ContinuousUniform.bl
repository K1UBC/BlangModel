package blang.distributions

import static java.lang.Math.log


import blang.core.RealVar

model ContinuousUniform {
  
  random RealVar realization
   
  param  RealVar min, 
                 max
  
  
  laws {
    
    logf(min, max) {
      - log(max - min)
    }
    
    indicator(realization, min, max) {
      min <= realization &&
      realization <= max
    }
    
  }
}