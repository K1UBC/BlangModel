package blang.examples

model TestMVN {
  
  random Matrix variables ?: dense(2,1) 
  
  param Matrix mean ?: dense(2,1) 
  
  laws {
    variables | mean,  
                CholeskyDecomposition precision = identity(2).denseCopy.cholesky 
              ~ MultivariateNormal(mean, precision)
  }
  
}