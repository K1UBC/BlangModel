package blang.validation.internals.fixtures

model Deep_Diri  {
  
  random DenseSimplex failureProbabilities ?: denseSimplex(2)
  
  random RealVar p0 ?: realVar
  param RealVar strength ?: constant(0.1)
  
  laws {
    
    p0 ~ ContinuousUniform(0.5, 1.0)
    failureProbabilities | p0, strength ~ Dirichlet(denseCopy(#[strength * p0, strength * (1.0 - p0)]))
    
  }
}